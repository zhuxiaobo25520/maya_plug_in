///////UI载入文字加空格//////////////////
global proc zxb_UI_string_kongge_proc(string$tempname)
{
string $sel[]=`ls -sl`;
string $obj="";
for($i=0;$i<size($sel);$i++)
    {
    $obj+=(" "+$sel[$i]);
    }
textFieldGrp -e -tx ($obj) $tempname;
}




/////////显示按钮按钮并清空文字///////
global proc zxb_UI_VS_open_proc(string$butt,string $tempname)
{

button -e -en 1 $butt;
textFieldGrp -e -tx "" $tempname;

}


/////////隐藏按钮并载入文字

global proc zxb_UI_VS_slose_proc(string $butt,string $textFieldGrpa)
{

button -e -en 0 $butt;
zxb_UI_string_kongge_proc($textFieldGrpa);
}
global proc zxb_public_proc()
{
    
   print "加载UI"; 
}
